<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:docs="clr-namespace:SkEditor.Controls.Docs"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="docs:DocEntryControl"
             x:Class="SkEditor.Controls.Docs.DocEntryControl">
   
    <Expander Margin="5" Content="{Binding EntryContent}" IsExpanded="{Binding IsExpanded}">
        <Expander.Header>
            <TextBlock Grid.Row="0"
                       FontSize="16" FontWeight="SemiBold" 
                       Text="{Binding EntryTitle}" />
        </Expander.Header>
    </Expander>
    
</UserControl>
